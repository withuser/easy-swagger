---
import BaseLayout from '@/layouts/BaseLayout.astro';
import projects from '@/data/open-api.json';
import Swagger from '@/components/Swagger';

export async function getStaticPaths() {
  return projects.map(({ title, version }) => ({
    params: { project: title, version },
  }));
}
const props = Astro.params;
---

<BaseLayout>
  <Swagger {...props} client:only="react" />
</BaseLayout>
